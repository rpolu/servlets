<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<welcome-file-list>
		<welcome-file>add.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ass</servlet-name>
		<servlet-class>com.prathima.AddingServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ass</servlet-name>
		<url-pattern>/as</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>os</servlet-name>
		<servlet-class>com.prathima.OutPutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>os</servlet-name>
	    <url-pattern>/ossss</url-pattern>
	</servlet-mapping>
</web-app>